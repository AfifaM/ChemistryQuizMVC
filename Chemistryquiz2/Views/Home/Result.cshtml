@model Chemistryquiz2.Models.Result

@{
    ViewBag.Title = "Result";

    var item = Model;

}


<br />
@if (item.QuizID == 1)
{
    <img src="~/Content/Images/chemistryjoke.jpg" alt="Chemistry joke" width="600" height="400" class="align-center" />
    <p class="description align-center">You scored @ViewBag.QuizScore out of 13.</p>
    <br />
}
<p class="description align-center">@Html.Raw(@item.Text)</p><br />
<br />



